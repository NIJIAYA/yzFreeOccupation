{
  "name": "project-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --max_old_space_size=102400",
    "build": "vue-cli-service build --max_old_space_size=102400",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "eslint-fix": "eslint src/**/**/*.vue --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "animate.css": "^4.1.0",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "css-loader": "^5.2.1",
    "echarts": "^5.3.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.26.0",
    "style-loader": "^1.3.0",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-animate-number": "^0.4.2",
    "vue-cli-configjs": "^2.0.0",
    "vue-loader": "^15.9.2",
    "vue-navigation": "^1.1.4",
    "vue-router": "^3.1.6",
    "vue-scroller": "^2.2.4",
    "vue-seamless-scroll": "^1.1.23",
    "weixin-js-sdk": "^1.6.0",
    "weui": "^2.4.4",
    "weui.js": "^1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-block-scoping": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.9.6",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "image-webpack-loader": "^6.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-extra-semi": "off",
      "no-multiple-empty-lines": "off",
      "no-new": "off",
      "spaced-comment": "off",
      "no-trailing-spaces": "off",
      "eol-last": "off",
      "import/first": "off",
      "quotes": "off",
      "semi": "off",
      "no-unused-vars": "warn",
      "eqeqeq": "off",
      "handle-callback-err": "off",
      "comma-dangle": "error",
      "camelcase": "off",
      "indent": "off",
      "space-before-function-paren": "off",
      "no-multi-spaces": "off",
      "one-var": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
